(library (vicare crypto openssl constants)
  (export

    ;; Message Digest 4
    MD4_DIGEST_LENGTH

    ;; Message Digest 5
    MD5_DIGEST_LENGTH

    ;; MDC2
    MDC2_DIGEST_LENGTH

    ;; SHA
    SHA_DIGEST_LENGTH
    SHA224_DIGEST_LENGTH
    SHA256_DIGEST_LENGTH
    SHA384_DIGEST_LENGTH
    SHA512_DIGEST_LENGTH

    ;; RIPEMD 160
    RIPEMD160_DIGEST_LENGTH
    )
  (import (rename (only (vicare syntactic-extensions)
			define-inline-constant)
		  (define-inline-constant	defc)))


;;;; constants

;; Message Digest 4
(defc MD4_DIGEST_LENGTH			@VALUEOF_MD4_DIGEST_LENGTH@)

;; Message Digest 5
(defc MD5_DIGEST_LENGTH			@VALUEOF_MD5_DIGEST_LENGTH@)

;; MDC2
(defc MDC2_DIGEST_LENGTH		@VALUEOF_MDC2_DIGEST_LENGTH@)

;; SHA
(defc SHA_DIGEST_LENGTH			@VALUEOF_SHA_DIGEST_LENGTH@)
(defc SHA224_DIGEST_LENGTH		@VALUEOF_SHA224_DIGEST_LENGTH@)
(defc SHA256_DIGEST_LENGTH		@VALUEOF_SHA256_DIGEST_LENGTH@)
(defc SHA384_DIGEST_LENGTH		@VALUEOF_SHA384_DIGEST_LENGTH@)
(defc SHA512_DIGEST_LENGTH		@VALUEOF_SHA512_DIGEST_LENGTH@)

;; RIPEMD 160
(defc RIPEMD160_DIGEST_LENGTH		@VALUEOF_RIPEMD160_DIGEST_LENGTH@)


;;;; done

)

